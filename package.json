  "version": "0.4.1",
  "description": "BF WP4 backend services",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "Siemens BrownField Project WP4 Team",
  "private": true,
  "engines": {
    "node": "^8.10.0",
    "npm": ">5.5.0 <=5.7.0",
    "yarn": "^1.7.0"
